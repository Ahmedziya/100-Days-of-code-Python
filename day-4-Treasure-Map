row1 = ["⬜️","⬜️","⬜️"]
row2 = ["⬜️","⬜️","⬜️"]
row3 = ["⬜️","⬜️","⬜️"]
map = [row1, row2, row3]
print(f"{row1}\n{row2}\n{row3}")
position = int(input("Where do you want to put the treasure? "))
# 🚨 Don't change the code above 👆

#Write your code below this row 👇

c = int((position/10)-1)
r = int((position%10)-1)
map[r][c] = "X"
#i swapped c and r 
#print (map[r][c])

# 11=0,0
# 12=1,0
# 13=2,0
# 21=0,1
# 22=1,1
# 23=2,1
# 31=0,2
# 32=1,2
# 33=2,2

